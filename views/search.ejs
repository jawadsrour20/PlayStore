    <script>
      window.onscroll = function() {myFunction()};
      
      var navbar = document.getElementById("navbar");
      var sticky = navbar.offsetTop;
      
      function myFunction() {
        if (window.pageYOffset >= sticky) {
          navbar.classList.add("sticky")
        } else {
          navbar.classList.remove("sticky");
        }
      }
      </script>

    <script>


      var currentSectionSelect = document.getElementById("show-sections");
      var applicationsSection = document.getElementById("apps-section");
      var gamesSection = document.getElementById("games-section");
      var moviesSection = document.getElementById("movies-section");
      var booksSection = document.getElementById("books-section");
      var currentSection = currentSectionSelect.value;

      function reRender()
      {
        currentSection = currentSectionSelect.value;

      if (currentSection === "0")
      {
          gamesSection.style.display = "block";
          moviesSection.style.display = "block";
          booksSection.style.display = "block";
          applicationsSection.style.display = "block";
      }
      else if(currentSection === "1")
        {
   
          gamesSection.style.display = "none";
          moviesSection.style.display = "none";
          booksSection.style.display = "none";
          applicationsSection.style.display = "block";
        }
      else if(currentSection === "2")
      {
       
          gamesSection.style.display = "block";
          moviesSection.style.display = "none";
          booksSection.style.display = "none";
          applicationsSection.style.display = "none";
      }
      else if (currentSection === "3")
      {
          gamesSection.style.display = "none";
          moviesSection.style.display = "block";
          booksSection.style.display = "none";
          applicationsSection.style.display = "none";
      }
      else if (currentSection === "4")
      {
          gamesSection.style.display = "none";
          moviesSection.style.display = "none";
          booksSection.style.display = "block";
          applicationsSection.style.display = "none";
      
      }
      }
    </script>

